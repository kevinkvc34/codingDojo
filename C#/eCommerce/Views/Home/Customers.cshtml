@model List<User>
<partial name="Nav"></partial>
<form action="/create-user"method="post">
<label>Customer Name</label>
<input type="text"name="customerName">
<input type="submit">
</form>
<table class="table">
    <tr>
        <th>Customer Name</th>
        <th>Date Created</th>
        <th>Action</th>
    </tr>
    @{
        foreach(var x in Model) {
            String date = $"{x.CreatedAt.Month}/{x.CreatedAt.Day}/{x.CreatedAt.Year}";
            <tr>
                <td>@x.FirstName @x.LastName</td>
                <td>@date</td>
                <td><a href="/delete-user/@x.UserId"class="btn btn-danger">Delete</a></td>
            </tr>
        }
    }
</table>