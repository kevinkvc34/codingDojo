@model LoginUser
<h1>Login</h1>
<form action="/login-user"method="post">
<label>Email</label>
<input asp-for="Email">
<span asp-validation-for="Email"></span>
<label>Password</label>
<input asp-for="Password">
<span asp-validation-for="Password"></span>
<input type="submit"class="btn btn-primary">
</form>